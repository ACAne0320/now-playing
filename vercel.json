{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PUBLIC_MODE": "true",
    "NOW_PLAYING_API_KEY": "@now_playing_api_key"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10
    }
  }
}
