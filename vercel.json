{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PUBLIC_MODE": "true"
  },
  "includeFiles": [
    "client/renderer/templates/**",
    "client/**"
  ]
}
